<div class="page-wrap">
	{{> molecules-ad-banner-block }}
	{{> organisms-header }}
	{{> organisms-primary-mega-menu }}
	<section class="l-2up--8-4">
		<div class="wrap">
			<div class="l-gi">
				{{> molecules-search }}
				<div class="tgtabs">
					<!-- start of Tabs component -->
					<div class="tgtabs__wrap">
						<div class="tgtabs__row">
							<div class="tgtab">
								<a href="#">All</a>
							</div>
							<div class="tgtab">
								<a href="#">Articles</a>
							</div>
							<div class="tgtab">
								<a href="#">Programmes &amp; Shows</a>
							</div>
							<div class="tgtab">
								<a href="#">Videos</a>
							</div>
						</div>
					</div>
					<div class="tgtab__targets">
						<div class="tgtab__target">
							<header class="vertical-listing__header">
								<h4 class="vertical-listing__title">99,999 Results</h4>
								{{> organisms-utility-bar}}

							</header>
							<ul class="vertical-list">
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item-portrait}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item-portrait}}</li>
								<li>{{> molecules-toggle-vertical-list-item-portrait}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
							</ul>
							<footer class="vertical-listing__footer">
								{{> molecules-pagination(article:true)}}
							</footer>
						</div>
						<div class="tgtab__target">
							<header class="vertical-listing__header">
								<h4 class="vertical-listing__title">99 Results</h4>
								{{> molecules-pagination(article:true)}}
								<div class="sort">
									<span>Sort:</span>
									<select class="dropdown">
										<option value="popular">Latest</option>
										<option value="latest">Option Two</option>
										<option value="latest">Option Three</option>
									</select>
								</div>
							</header>
							<ul class="vertical-list">
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
							</ul>
							<footer class="vertical-listing__footer">
								{{> molecules-pagination(article:true)}}
							</footer>
						</div>
						<div class="tgtab__target">
							<header class="vertical-listing__header">
								<h4 class="vertical-listing__title">99 Results</h4>
								{{> molecules-pagination(article:true)}}
								<div class="sort">
									<span>Sort:</span>
									<select class="dropdown">
										<option value="popular">Latest</option>
										<option value="latest">Option Two</option>
										<option value="latest">Option Three</option>
									</select>
								</div>
							</header>
							<ul class="vertical-list">
								<li>{{> molecules-toggle-vertical-list-item-portrait}}</li>
								<li>{{> molecules-toggle-vertical-list-item-portrait}}</li>
								<li>{{> molecules-toggle-vertical-list-item-portrait}}</li>
								<li>{{> molecules-toggle-vertical-list-item-portrait}}</li>
								<li>{{> molecules-toggle-vertical-list-item-portrait}}</li>
							</ul>
							<footer class="vertical-listing__footer">
								{{> molecules-pagination(article:true)}}
							</footer>
						</div>
						<div class="tgtab__target">
							<header class="vertical-listing__header">
								<h4 class="vertical-listing__title">999 Results</h4>
								{{> molecules-pagination(article:true)}}
								<div class="sort">
									<span>Sort:</span>
									<select class="dropdown">
										<option value="popular">Latest</option>
										<option value="latest">Option Two</option>
										<option value="latest">Option Three</option>
									</select>
								</div>
							</header>
							<ul class="vertical-list">
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
								<li>{{> molecules-toggle-vertical-list-item}}</li>
							</ul>
							<footer class="vertical-listing__footer">
								{{> molecules-pagination(article:true)}}
							</footer>
						</div>
					</div>
				</div>
				<!-- end of Tabs component -->
			</div>
			<aside class="l-gi show-only-desktop">
				<!-- start of Ad-300x100-block component -->
				{{> molecules-ad-300x250-block }}
				<!-- end of Ad-300x100-block component -->
			</aside>
		</div>
	</section>
	{{> organisms-footer }}
</div>