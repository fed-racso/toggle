<div class="page-wrap">
  {{> molecules-ad-banner-block }}
  {{> organisms-header }}
  {{> organisms-primary-mega-menu }}
  {{> molecules-section-header(section-header__title: "Korean Buzz", section-header__parent: "Entertainment", section-header__has-logo:false) }}
  <section>
    <div class="wrap">
      <div class="l-2up--8-4">
        <div class="l-gi">
          {{> organisms-article (card-class:card--background-no-padding)}}
          {{> organisms-content-grid(rows: 3, component-title: More From Korean Buzz, card-class:card--background-no-padding) }}
          {{> organisms-content-grid(rows: 3, component-title: More From Entertainment, card-class:card--background-no-padding) }}
        </div>
        <div class="l-gi split-grid">
          {{> molecules-ad-300x250-block(split-index:0) }}
          {{> molecules-fluid-height(fluid-height-title: Most Popular, card-class:card) }}
          {{> molecules-ad-300x250-block }}
        </div>
      </div>
    </div>
  </section>
    {{> organisms-footer }}
</div>
